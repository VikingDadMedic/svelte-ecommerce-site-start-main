{
	"name": "svelte-ecommerce-site",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:graphql": "export $(grep ^PUBLIC_DIRECTUS_PROJECT_URL .env | xargs) && graphql-codegen --config codegen.json"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.3.0",
		"@fontsource/fira-sans": "^4.5.10",
		"@fontsource/montserrat": "^4.5.13",
		"@graphql-codegen/cli": "^2.16.1",
		"@graphql-codegen/introspection": "^2.2.3",
		"@graphql-codegen/typescript": "^2.8.5",
		"@graphql-codegen/typescript-document-nodes": "^2.3.10",
		"@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
		"@graphql-codegen/typescript-operations": "^2.5.10",
		"@iconify/svelte": "^3.0.1",
		"@jridgewell/sourcemap-codec": "^1.4.14",
		"@js-temporal/polyfill": "^0.4.3",
		"@sveltejs/adapter-auto": "1.0.0-next.91",
		"@sveltejs/kit": "1.0.0-next.589",
		"@types/node": "^18.11.16",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"commitlint": "^17.3.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"schema-dts": "^1.1.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "^4.10.7",
		"tiny-invariant": "^1.3.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^4.0.1"
	},
	"type": "module",
	"peerDependencies": {
		"@types/node": "^18.11.15",
		"ts-node": "^10.9.1"
	}
}
